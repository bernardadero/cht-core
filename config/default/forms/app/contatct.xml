<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Contact</h:title>
    <model>
      <instance>
        <contatct delimiter="#" id="contact" prefix="J1!contact!" version="2020-06-18 19:44:34">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <contact>
                <_id/>
                <name/>
              </contact>
              <parent>
                <parent>
                  <contact>
                    <name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <source/>
          <source_id/>
          <place_id/>
          <place_name/>
          <head/>
          <location_nformation>
            <county/>
            <sub_county/>
            <ward/>
            <implementing_partner/>
            <date_of_first_contact/>
            <unique_identifier/>
            <population_type/>
          </location_nformation>
          <bio_nformation>
            <first_name/>
            <middle_name/>
            <sure_name/>
            <alias/>
            <sex/>
            <date_of_birth/>
            <age/>
            <phone_number/>
            <alt_contact_person/>
            <alt_contatc_phone_number/>
            <contacted_by_the_peer/>
            <program_receiving_service_from/>
            <where_do_you_operate/>
            <hotspot_type/>
            <other_cadre/>
            <year_start_sex_work/>
            <year_strted_sex_with_men/>
            <year_started_using_drugs/>
            <average_sex_per_week/>
            <average_anal_sex_per_week/>
            <average_drug_injection_per_week/>
          </bio_nformation>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </contatct>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/contatct/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/contatct/inputs/source" type="string"/>
      <bind nodeset="/contatct/inputs/source_id" type="string"/>
      <bind nodeset="/contatct/inputs/contact/_id" type="db:clinic"/>
      <bind nodeset="/contatct/inputs/contact/name" type="string"/>
      <bind nodeset="/contatct/inputs/contact/contact/_id" type="db:person"/>
      <bind nodeset="/contatct/inputs/contact/contact/name" type="string"/>
      <bind nodeset="/contatct/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/contatct/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind calculate="../inputs/source" nodeset="/contatct/source" type="string"/>
      <bind calculate="../inputs/source_id" nodeset="/contatct/source_id" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/contatct/place_id" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/contatct/place_name" type="string"/>
      <bind calculate="../inputs/contact/contact/name" nodeset="/contatct/head" type="string"/>
      <bind nodeset="/contatct/location_nformation/county" type="string"/>
      <bind nodeset="/contatct/location_nformation/sub_county" type="string"/>
      <bind nodeset="/contatct/location_nformation/ward" type="string"/>
      <bind nodeset="/contatct/location_nformation/implementing_partner" type="string"/>
      <bind nodeset="/contatct/location_nformation/date_of_first_contact" type="date"/>
      <bind nodeset="/contatct/location_nformation/unique_identifier" type="string"/>
      <bind nodeset="/contatct/location_nformation/population_type" required="true()" type="select1"/>
      <bind nodeset="/contatct/bio_nformation/first_name" type="string"/>
      <bind nodeset="/contatct/bio_nformation/middle_name" type="string"/>
      <bind nodeset="/contatct/bio_nformation/sure_name" type="string"/>
      <bind nodeset="/contatct/bio_nformation/alias" type="string"/>
      <bind nodeset="/contatct/bio_nformation/sex" required="false()" type="select1"/>
      <bind nodeset="/contatct/bio_nformation/date_of_birth" type="date"/>
      <bind nodeset="/contatct/bio_nformation/age" type="string"/>
      <bind nodeset="/contatct/bio_nformation/phone_number" type="string"/>
      <bind nodeset="/contatct/bio_nformation/alt_contact_person" required="false()" type="string"/>
      <bind nodeset="/contatct/bio_nformation/alt_contatc_phone_number" type="string"/>
      <bind nodeset="/contatct/bio_nformation/contacted_by_the_peer" required="false()" type="select1"/>
      <bind nodeset="/contatct/bio_nformation/program_receiving_service_from" relevant=" /contatct/bio_nformation/contacted_by_the_peer =&quot;yes&quot;" type="string"/>
      <bind nodeset="/contatct/bio_nformation/where_do_you_operate" type="string"/>
      <bind nodeset="/contatct/bio_nformation/hotspot_type" required="false()" type="select1"/>
      <bind nodeset="/contatct/bio_nformation/other_cadre" relevant=" /contatct/bio_nformation/hotspot_type =&quot;other&quot;" type="string"/>
      <bind nodeset="/contatct/bio_nformation/year_start_sex_work" type="string"/>
      <bind nodeset="/contatct/bio_nformation/year_strted_sex_with_men" relevant=" /contatct/location_nformation/population_type =&quot;msm&quot;" type="string"/>
      <bind nodeset="/contatct/bio_nformation/year_started_using_drugs" type="string"/>
      <bind nodeset="/contatct/bio_nformation/average_sex_per_week" type="string"/>
      <bind nodeset="/contatct/bio_nformation/average_anal_sex_per_week" type="string"/>
      <bind nodeset="/contatct/bio_nformation/average_drug_injection_per_week" relevant=" /contatct/location_nformation/population_type =&quot;pwd&quot;" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/contatct/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/contatct/inputs">
      <label>Patient</label>
      <group ref="/contatct/inputs/contact">
        <label>Contact</label>
        <input appearance="db-object" ref="/contatct/inputs/contact/_id">
          <label>What is the patient's name?</label>
        </input>
        <group ref="/contatct/inputs/contact/contact">
          <label>Contact</label>
          <input appearance="db-object" ref="/contatct/inputs/contact/contact/_id">
            <label>ID of head of household</label>
          </input>
        </group>
        <group ref="/contatct/inputs/contact/parent">
          <label>Parent</label>
          <group ref="/contatct/inputs/contact/parent/parent">
            <label>Parent</label>
            <group ref="/contatct/inputs/contact/parent/parent/contact">
              <label>Contact</label>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/contatct/location_nformation">
      <label>General Information</label>
      <input ref="/contatct/location_nformation/county">
        <label>County</label>
      </input>
      <input ref="/contatct/location_nformation/sub_county">
        <label>Sub County</label>
      </input>
      <input ref="/contatct/location_nformation/ward">
        <label>Ward</label>
      </input>
      <input ref="/contatct/location_nformation/implementing_partner">
        <label>Implementing Partner:</label>
      </input>
      <input ref="/contatct/location_nformation/date_of_first_contact">
        <label>Date of first contact:</label>
      </input>
      <input ref="/contatct/location_nformation/unique_identifier">
        <label>Unique Identifier Code:</label>
      </input>
      <select1 ref="/contatct/location_nformation/population_type">
        <label>Key Population Type:</label>
        <item>
          <label>1. FSW</label>
          <value>fsw</value>
        </item>
        <item>
          <label>2. MSM</label>
          <value>msm</value>
        </item>
        <item>
          <label>3. MSW</label>
          <value>msw</value>
        </item>
        <item>
          <label>4. PWID</label>
          <value>pwid</value>
        </item>
        <item>
          <label>5. PWUD</label>
          <value>pwud</value>
        </item>
        <item>
          <label>6. Transman</label>
          <value>transman</value>
        </item>
        <item>
          <label>7. Transwoman</label>
          <value>transwoman</value>
        </item>
      </select1>
    </group>
    <group appearance="field-list" ref="/contatct/bio_nformation">
      <label>Bio Information</label>
      <input ref="/contatct/bio_nformation/first_name">
        <label>First Name:</label>
      </input>
      <input ref="/contatct/bio_nformation/middle_name">
        <label>Middle Name:</label>
      </input>
      <input ref="/contatct/bio_nformation/sure_name">
        <label>Last Name:</label>
      </input>
      <input ref="/contatct/bio_nformation/alias">
        <label>Alias</label>
      </input>
      <select1 ref="/contatct/bio_nformation/sex">
        <label>Sex</label>
        <item>
          <label>Male</label>
          <value>male</value>
        </item>
        <item>
          <label>Female</label>
          <value>female</value>
        </item>
      </select1>
      <input ref="/contatct/bio_nformation/date_of_birth">
        <label>Date of Birth:</label>
      </input>
      <input ref="/contatct/bio_nformation/age">
        <label>Age:</label>
      </input>
      <input ref="/contatct/bio_nformation/phone_number">
        <label>Contact Phone number</label>
      </input>
      <input ref="/contatct/bio_nformation/alt_contact_person">
        <label>Alternate Contact Person:</label>
      </input>
      <input ref="/contatct/bio_nformation/alt_contatc_phone_number">
        <label>Alternate Contact Person Phone Number</label>
      </input>
      <select1 ref="/contatct/bio_nformation/contacted_by_the_peer">
        <label>Have you been contacted by a peer educator?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/contatct/bio_nformation/program_receiving_service_from">
        <label>If yes, which program do you recieve services from?</label>
      </input>
      <input ref="/contatct/bio_nformation/where_do_you_operate">
        <label>Hotspot mostly freqented:</label>
      </input>
      <select1 ref="/contatct/bio_nformation/hotspot_type">
        <label>Type of spot mostly frequented?</label>
        <item>
          <label>1. Street</label>
          <value>street</value>
        </item>
        <item>
          <label>2.Injecting Den</label>
          <value>injecting_den</value>
        </item>
        <item>
          <label>3. Uninhabitable building</label>
          <value>uninhabited_building</value>
        </item>
        <item>
          <label>4. Parks</label>
          <value>parks</value>
        </item>
        <item>
          <label>5. Homes</label>
          <value>homes</value>
        </item>
        <item>
          <label>6. Beach</label>
          <value>beach</value>
        </item>
        <item>
          <label>7. Casino</label>
          <value>casino</value>
        </item>
        <item>
          <label>8. Bar with lodging</label>
          <value>bar_with_lodging</value>
        </item>
        <item>
          <label>9. Bar without lodging</label>
          <value>bar_without_lodging</value>
        </item>
        <item>
          <label>10. Sex den</label>
          <value>sex_den</value>
        </item>
        <item>
          <label>11, Strip Club</label>
          <value>strip_club</value>
        </item>
        <item>
          <label>12. Highway</label>
          <value>highway</value>
        </item>
        <item>
          <label>13. Brothel</label>
          <value>brothel</value>
        </item>
        <item>
          <label>14. Guest House / Hotals/ Lodgings</label>
          <value>guest_house_hotels</value>
        </item>
        <item>
          <label>15. Massage Parlor</label>
          <value>massage_parlor</value>
        </item>
        <item>
          <label>16. Changaa Parlor</label>
          <value>changaa_den</value>
        </item>
        <item>
          <label>17. Barbershop /Salon</label>
          <value>barbershop</value>
        </item>
        <item>
          <label>18. Virtual Space</label>
          <value>virtual_space</value>
        </item>
        <item>
          <label>19. Other (Specify)</label>
          <value>other</value>
        </item>
      </select1>
      <input ref="/contatct/bio_nformation/other_cadre">
        <label>Specify other type of hotspot:</label>
      </input>
      <input ref="/contatct/bio_nformation/year_start_sex_work">
        <label>What year did you start sex work?</label>
      </input>
      <input ref="/contatct/bio_nformation/year_strted_sex_with_men">
        <label>Which year did you start having sex with men?</label>
      </input>
      <input ref="/contatct/bio_nformation/year_started_using_drugs">
        <label>Which year did you start using using drugs(injecting or smoking)?</label>
      </input>
      <input ref="/contatct/bio_nformation/average_sex_per_week">
        <label>On average, how many sex acts do you have per week?</label>
      </input>
      <input ref="/contatct/bio_nformation/average_anal_sex_per_week">
        <label>On average, how many anal sex acts do you have per week?</label>
      </input>
      <input ref="/contatct/bio_nformation/average_drug_injection_per_week">
        <label>On average, how many times do you inject drugs per day?</label>
      </input>
    </group>
  </h:body>
</h:html>
