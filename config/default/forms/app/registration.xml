      <bind nodeset="/registration/inputs/contact/contact/name" type="string"/>
      <bind nodeset="/registration/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/registration/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind calculate="../inputs/source" nodeset="/registration/source" type="string"/>
      <bind calculate="../inputs/source_id" nodeset="/registration/source_id" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/registration/place_id" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/registration/place_name" type="string"/>
      <bind calculate="../inputs/contact/contact/name" nodeset="/registration/head" type="string"/>
      <bind nodeset="/registration/id_numbers/patient_nationalIdnumber" type="string"/>
